    "pg-hstore": "^2.3.2",
    "sequelize": "^4.0.0",
    "mixpanel-browser": "^2.11.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.5.4",
    "react-mixpanel": "0.0.8",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1"
